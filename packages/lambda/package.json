{
  "name": "@serverless-chrome/lambda",
  "version": "1.0.0-5",
  "description": "Run headless Chrome/Chromium on AWS Lambda",
  "author": "Marco LÃ¼thy",
  "keywords": [
    "serverless",
    "chrome",
    "chromium",
    "headless",
    "aws",
    "lambda"
  ],
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.es.js",
  "files": [
    "dist/bundle.cjs.js",
    "dist/bundle.es.js",
    "scripts/postinstall.js"
  ],
  "repository": "git@github.com:adieuadieu/serverless-chrome.git",
  "bugs": {
    "url": "https://github.com/adieuadieu/serverless-chrome/issues"
  },
  "homepage": "https://github.com/adieuadieu/serverless-chrome/tree/master/packages/lambda",
  "license": "MIT",
  "engines": {
    "node": ">= 6.10"
  },
  "config": {
    "jsSrc": "src/"
  },
  "scripts": {
    "test": "npm run lint && nyc ava",
    "watch:test": "ava --watch",
    "test:docker": "docker run -v \"$PWD/integration-test\":/var/task -v \"$PWD/dist\":/var/task/dist lambci/lambda:nodejs6.10 handler.run",
    "ava": "ava",
    "lint": "npm run lint:eslint -s",
    "lint:eslint": "eslint $npm_package_config_jsSrc",
    "coverage": "nyc report",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "rm -Rf dist/ && yarn build",
    "postinstall": "node scripts/postinstall.js",
    "build-binaries": "scripts/build-binaries.sh",
    "build-binaries:chromium": "scripts/build-binaries.sh chromium",
    "latest-versions": "scripts/latest-versions.sh",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact"
  },
  "dependencies": {
    "extract-zip": "1.6.5"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-node-resolve": "3.0.0"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "6.10"
          }
        }
      ]
    ]
  }
}
